<?xml version="1.0" encoding="utf-8"?>
<VisualStudioToolFile
	Name="YASM"
	Version="8.00"
	>
	<Rules>
		<CustomBuildRule
			Name="YASM"
			DisplayName="yasm"
			CommandLine="..\..\tools\bin\yasm.exe -f win32 -Xvc -o $(IntDir)/$(InputName).obj $(InputDir)/$(InputName).asm"
			Outputs="$(IntDir)/$(InputName).obj"
			FileExtensions="*.asm"
			>
			<Properties>
			</Properties>
		</CustomBuildRule>
	</Rules>
</VisualStudioToolFile>
